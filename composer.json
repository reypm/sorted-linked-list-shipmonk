{
  "name": "reypm/sorted-linked-list",
  "description": "Type-safe sorted linked list implementation for PHP 8.4+",
  "type": "library",
  "keywords": [
    "sorted",
    "linked-list",
    "data-structure",
    "collection"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Reynier Perez",
      "email": "reynierpm@gmail.com"
    }
  ],
  "require": {
    "php": "^8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.48"
  },
  "autoload": {
    "psr-4": {
      "SortedList\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SortedList\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "phpstan": "phpstan analyse src tests --level=max",
    "cs-fix": "php-cs-fixer fix",
    "cs-check": "php-cs-fixer fix --dry-run --diff"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}